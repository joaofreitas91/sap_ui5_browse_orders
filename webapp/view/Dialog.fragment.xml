<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleConfirm">
		<!-- <groupItems>
			<ViewSettingsItem text="Field 1" key="1" selected="true" />
			<ViewSettingsItem text="Field 2" key="2" />
			<ViewSettingsItem text="Field 3" key="3" />
		</groupItems> -->
		<filterItems>
			<ViewSettingsFilterItem text="Orders" items="{modelOrders>/}" key="Order" >
				<items >
					<ViewSettingsItem text="{modelOrders>OrderID}" key="{modelOrders>OrderID}" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem text="Shipped" items="{modelOrders>/}" key="Shipped">
				<items>
					<ViewSettingsItem text="{modelOrders>ShippedDate}" key="{modelOrders>ShippedDate}" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>